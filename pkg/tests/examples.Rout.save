> printRounded <- function( x ) {
+    for( i in names( x ) ) {
+       cat ( "$", i, "\n", sep = "" )
+       if( is.numeric( x[[i]] ) ) {
+          print( round( x[[i]], 3 ) )
+       } else {
+          print( x[[i]] )
+       }
+       cat( "\n" )
+    }
+    cat( "attr(,\"class\")\n" )
+    print( class( x ) )
+ }
> 
> # round gradients to increase reproducibility of the accuracy
> roundGradients <- function( object ) {
+    object$gradient <- round( object$gradient, 3 )
+    return( object )   
+ }
